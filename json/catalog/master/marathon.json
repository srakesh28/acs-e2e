{
  "id": "/catalog/catalogservice",
  "cmd": null,
  "cpus": 0.25,
  "mem": 128,
  "disk": 0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "flakio/catalogservice:8",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 0,
          "servicePort": 10005,
          "protocol": "tcp",
          "name": "http",
          "labels": {
            "VIP_0": "192.168.2.1:80"
          }
        }
      ],
      "privileged": false,
      "parameters": [],
      "forcePullImage": false
    }
  },
  "env": {
    "ELASTICSEARCH_PORT": "http://elasticsearch-executor.elasticsearch.mesos:1025",
    "server.urls": "http://0.0.0.0:80"
  },
  "portDefinitions": [
    {
      "port": 10005,
      "protocol": "tcp",
      "labels": {}
    }
  ]
}
